#Given a number N, print the sum of the squares of its digits.
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int N = obj.nextInt();
        int sum = 0;
        while(N > 0)
        {
           int res = N % 10;
           sum += res * res;
           N /= 10;
        }
        System.out.print(sum);
    }
}
