#Given a number N, print 'yes' if it is composite else print 'no'.
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int n = obj.nextInt();
        for(int i=2;i<n;i++)
        {
            if(n%i==0)
            {
                System.out.println("yes");
                return;
            }
        }
        System.out.println("no");
    }
}
