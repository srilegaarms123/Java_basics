#Given 3 numbers a,b,c print a*b mod c.
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int a = obj.nextInt();
        int b = obj.nextInt();
        int c = obj.nextInt();
        int res = a * b;
        int fin = res % c;
        System.out.print(fin);
    }
}
