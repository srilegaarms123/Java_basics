#Print the First 3 multiples of the given number "N". (N is a positive integer) Note: print the characters with a single space between them.
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int N = obj.nextInt();
        for (int i = 1; i <= 3; i++) {
            System.out.print(N * i);
            if (i < 3) {
                System.out.print(" "); 
           }
        }
    }
}
